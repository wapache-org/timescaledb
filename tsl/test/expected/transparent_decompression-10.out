-- this should use DecompressChunk node
:PREFIX SELECT * FROM metrics WHERE device_id = 1 ORDER BY time LIMIT 5;
                                                  QUERY PLAN                                                  
--------------------------------------------------------------------------------------------------------------
 Limit (actual rows=5 loops=1)
   ->  Custom Scan (ChunkAppend) on metrics (actual rows=5 loops=1)
         Order: metrics."time"
         ->  Sort (actual rows=5 loops=1)
               Sort Key: _hyper_1_1_chunk."time"
               Sort Method: top-N heapsort 
               ->  Custom Scan (DecompressChunk) on _hyper_1_1_chunk (actual rows=1344 loops=1)
                     Filter: (device_id = 1)
                     ->  Seq Scan on compress_hyper_2_4_chunk (actual rows=2 loops=1)
                           Filter: (device_id = 1)
                           Rows Removed by Filter: 8
         ->  Index Scan Backward using _hyper_1_2_chunk_metrics_time_idx on _hyper_1_2_chunk (never executed)
               Filter: (device_id = 1)
         ->  Sort (never executed)
               Sort Key: _hyper_1_3_chunk."time"
               ->  Custom Scan (DecompressChunk) on _hyper_1_3_chunk (never executed)
                     Filter: (device_id = 1)
                     ->  Seq Scan on compress_hyper_2_5_chunk (never executed)
                           Filter: (device_id = 1)
